{
  "extends": [
    "next",
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "react/prop-types": "off",
    "no-extra-semi": "off",
    "react/display-name": "off",
    "no-undef": "off",
    "react-hooks/exhaustive-deps": "off",
    "jsx-ally/alt-text": "off",
    "@next/next/no-img-element": "off"
  },
  "globals": {
    "React": "writable"
  }
}
